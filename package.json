{
    "private": true,
    "scripts": {
        "format": "prettier -w .",
        "dev": "mix",
        "watch": "mix watch",
        "hot": "mix watch --hot",
        "prod": "mix --production",
        "start": "yarn watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "setup": "yarn dev && yarn admin:dev && yarn auth:dev",
        "admin:dev": "cross-env BUILD_PRESET=admin yarn dev",
        "admin:watch": "cross-env BUILD_PRESET=admin yarn watch",
        "admin:hot": "cross-env BUILD_PRESET=admin yarn hot",
        "admin:prod": "cross-env BUILD_PRESET=admin yarn prod",
        "admin:start": "cross-env BUILD_PRESET=admin yarn start",
        "admin:watch-poll": "cross-env BUILD_PRESET=admin yarn watch-poll",
        "auth:dev": "cross-env BUILD_PRESET=auth yarn dev",
        "auth:watch": "cross-env BUILD_PRESET=auth yarn watch",
        "auth:hot": "cross-env BUILD_PRESET=auth yarn hot",
        "auth:prod": "cross-env BUILD_PRESET=auth yarn prod",
        "auth:start": "cross-env BUILD_PRESET=auth yarn start",
        "auth:watch-poll": "cross-env BUILD_PRESET=auth yarn watch-poll",
        "landings:dev": "cross-env BUILD_PRESET=landings yarn dev",
        "landings:watch": "cross-env BUILD_PRESET=landings yarn watch",
        "landings:hot": "cross-env BUILD_PRESET=landings yarn hot",
        "landings:prod": "cross-env BUILD_PRESET=landings yarn prod",
        "landings:start": "cross-env BUILD_PRESET=landings yarn start",
        "landings:watch-poll": "cross-env BUILD_PRESET=landings yarn watch-poll",
        "build": "rm -rf public/apps && yarn prod & yarn admin:prod",
        "prepare": "husky install && cd ./resources/apidocs && yarn install",
        "test": "jest",
        "lint": "eslint --cache --ext \".ts,.tsx\" ./resources/ts",
        "lint:fix": "yarn lint --fix",
        "coverage:build": "yarn test --collect-coverage --coverage",
        "coverage:serve": "serve ./public/coverage/lcov-report",
        "coverage:start": "yarn coverage:build && yarn coverage:serve",
        "apidoc:build": "cd ./resources/apidocs && yarn build",
        "apidoc:watch": "cd ./resources/apidocs && yarn watch",
        "apidoc:serve": "cd ./resources/apidocs && yarn serve",
        "apidoc:start": "cd ./resources/apidocs && yarn start",
        "backup": "aws s3 mv s3://apparel-cdn/apps/robograding/latest s3://apparel-cdn/apps/robograding/v$(node -e \"process.stdout.write(require('./package.json').version)\") --recursive",
        "deploy": "aws s3 cp ./public/apps s3://apparel-cdn/apps/robograding/latest/apps --recursive"
    },
