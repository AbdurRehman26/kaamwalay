        "sass": "^1.37.5",
        "sass-loader": "^12.1.0",
        "serve": "^12.0.0",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.4",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.50.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.7.2"
    }
}
