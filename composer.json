{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "algolia/scout-extended": "^1.20",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "codeitnowin/barcode": "^3.0",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "laravel-notification-channels/pusher-push-notifications": "^2.3",
        "laravel/cashier": "^13.4",
        "laravel/framework": "^8.52",
        "laravel/horizon": "^5.7",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "paypal/paypal-checkout-sdk": "^1.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.8",
        "spatie/dropbox-api": "^1.19",
        "spatie/image": "1.10.5",
        "spatie/laravel-activitylog": "^4.2",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-query-builder": "^3.5",
        "spatie/laravel-webhook-client": "^3.0",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.11",
        "fakerphp/faker": "^1.15",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel-shift/blueprint": "^1.24",
        "laravel/sail": "^1.9",
        "laravel/telescope": "^4.6",
        "nunomaduro/collision": "^5.6",
        "nunomaduro/larastan": "^1.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-mock": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Services/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
