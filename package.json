{
    "private": true,
    "scripts": {
        "format": "prettier -w .",
        "dev": "mix",
        "watch": "mix watch",
        "hot": "mix watch --hot",
        "prod": "mix --production",
        "start": "yarn watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "admin:dev": "cross-env BUILD_PRESET=admin yarn dev",
        "admin:watch": "cross-env BUILD_PRESET=admin yarn watch",
        "admin:hot": "cross-env BUILD_PRESET=admin yarn hot",
        "admin:prod": "cross-env BUILD_PRESET=admin yarn prod",
        "admin:start": "cross-env BUILD_PRESET=admin yarn start",
        "admin:watch-poll": "cross-env BUILD_PRESET=admin yarn watch-poll",
        "build": "rm -rf public/apps && yarn prod & yarn admin:prod",
        "prepare": "husky install",
        "test": "jest",
        "coverage": "yarn test --collect-coverage --coverage",
        "coverage:serve": "serve ./coverage/jest/lcov-report",
        "apidoc:build": "apidoc -i resources/apidocs/src -o resources/apidocs/output",
        "apidoc:serve": "serve ./resources/apidocs/output",
        "apidoc:start": "yarn apidoc:build && yarn apidoc:serve",
        "backup": "aws s3 mv s3://apparel-cdn/apps/robograding/latest s3://apparel-cdn/apps/robograding/v$(node -e \"process.stdout.write(require('./package.json').version)\") --recursive",
        "deploy": "aws s3 cp ./public/apps s3://apparel-cdn/apps/robograding/latest/apps --recursive"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.2",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@reduxjs/toolkit": "^1.6.1",
        "axios": "^0.21.1",
        "card-validator": "^8.1.1",
        "class-transformer": "^0.4.0",
        "classnames": "^2.3.1",
        "debug": "^4.3.2",
        "immutable": "^4.0.0-rc.14",
        "inversify": "^5.1.1",
        "lodash": "^4.17.19",
        "moment": "^2.29.1",
        "object-hash": "^2.2.0",
        "polished": "^4.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-number-format": "^4.6.4",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "reflect-metadata": "^0.1.13",
        "swr": "^0.5.6",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@svgr/webpack": "^5.5.0",
        "@trivago/prettier-plugin-sort-imports": "^2.0.2",
        "@types/debug": "^4.1.7",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.0.0",
        "@types/object-hash": "^2.1.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.8",
        "apidoc": "^0.29.0",
        "browser-sync": "^2.27.5",
        "browser-sync-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0.3",
        "husky": "^7.0.0",
        "jest": "^27.0.6",
        "laravel-mix": "^6.0.27",
        "laravel-mix-make-file-hash": "^2.2.0",
        "postcss": "^8.1.14",
        "prettier": "^2.3.2",
        "pretty-quick": "^3.1.1",
        "sass": "^1.37.5",
        "sass-loader": "^12.1.0",
        "serve": "^12.0.0",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.4",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5",
        "url-loader": "^4.1.1"
    }
}
