{
    "private": true,
    "scripts": {
        "format": "prettier -w .",
        "dev": "mix",
        "watch": "mix watch",
        "hot": "mix watch --hot",
        "prod": "mix --production",
        "start": "yarn watch",
        "build:icons": "yarn svgo -f resources/assets/icons/payments -o resources/ts/shared/assets/icons/payments",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "setup": "yarn dev && yarn admin:dev && yarn auth:dev",
        "admin:dev": "cross-env BUILD_PRESET=admin yarn dev",
        "admin:watch": "cross-env BUILD_PRESET=admin yarn watch",
        "admin:hot": "cross-env BUILD_PRESET=admin yarn hot",
        "admin:prod": "cross-env BUILD_PRESET=admin yarn prod",
        "admin:start": "cross-env BUILD_PRESET=admin yarn start",
        "admin:watch-poll": "cross-env BUILD_PRESET=admin yarn watch-poll",
        "auth:dev": "cross-env BUILD_PRESET=auth yarn dev",
        "auth:watch": "cross-env BUILD_PRESET=auth yarn watch",
        "auth:hot": "cross-env BUILD_PRESET=auth yarn hot",
        "auth:prod": "cross-env BUILD_PRESET=auth yarn prod",
        "auth:start": "cross-env BUILD_PRESET=auth yarn start",
        "auth:watch-poll": "cross-env BUILD_PRESET=auth yarn watch-poll",
        "build": "rm -rf public/apps && yarn prod & yarn admin:prod",
        "prepare": "husky install",
        "test": "jest",
        "coverage:build": "yarn test --collect-coverage --coverage",
        "coverage:serve": "serve ./public/coverage/lcov-report",
        "coverage:start": "yarn coverage:build && yarn coverage:serve",
        "apidoc:build": "apidoc -i resources/apidocs -o public/apidocs",
        "apidoc:serve": "serve public/apidocs",
        "apidoc:start": "yarn apidoc:build && yarn apidoc:serve",
        "backup": "aws s3 mv s3://apparel-cdn/apps/robograding/latest s3://apparel-cdn/apps/robograding/v$(node -e \"process.stdout.write(require('./package.json').version)\") --recursive",
        "deploy": "aws s3 cp ./public/apps s3://apparel-cdn/apps/robograding/latest/apps --recursive"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.2",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@reduxjs/toolkit": "^1.6.1",
        "@stripe/react-stripe-js": "^1.4.1",
        "@stripe/stripe-js": "^1.16.0",
        "@types/lodash-es": "^4.17.4",
        "@types/react-instantsearch-dom": "^6.12.0",
        "@types/react-stripe-elements": "^6.0.5",
        "algoliasearch": "^4.10.3",
        "axios": "^0.21.1",
        "card-validator": "^8.1.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "classnames": "^2.3.1",
        "debug": "^4.3.2",
        "formik": "^2.2.9",
        "hash.js": "^1.1.7",
        "immutable": "^4.0.0-rc.14",
        "inversify": "^5.1.1",
        "localforage": "^1.9.0",
        "lodash-es": "^4.17.21",
        "moment": "^2.29.1",
        "object-hash": "^2.2.0",
        "polished": "^4.1.3",
        "react": "^17.0.2",
        "react-awesome-lightbox": "^1.8.0",
        "react-dom": "^17.0.2",
        "react-instantsearch-dom": "^6.12.1",
        "react-number-format": "^4.6.4",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-slick": "^0.28.1",
        "reflect-metadata": "^0.1.13",
        "slick-carousel": "^1.8.1",
        "svgo": "^2.3.1",
        "swr": "^0.5.6",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@svgr/webpack": "^5.5.0",
        "@trivago/prettier-plugin-sort-imports": "^2.0.2",
        "@types/debug": "^4.1.7",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.0.0",
        "@types/object-hash": "^2.1.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.8",
        "@types/react-slick": "^0.23.5",
        "apidoc": "^0.29.0",
        "browser-sync": "^2.27.5",
        "browser-sync-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0.3",
        "husky": "^7.0.0",
        "jest": "^27.0.6",
        "laravel-mix": "^6.0.27",
        "laravel-mix-make-file-hash": "^2.2.0",
        "lodash-webpack-plugin": "^0.11.6",
        "postcss": "^8.1.14",
